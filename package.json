{
  "name": "log2trace-viewer",
  "version": "1.0.0",
  "description": "An interactive visualization library for OpenTelemetry trace data",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "build": "tsc && esbuild src/index.ts --bundle --minify --sourcemap --metafile=dist/meta.json --format=esm --outfile=dist/index.js --loader:.css=text",
    "check": "tsc --noEmit",
    "demo": "npm run build && node demo/server.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "opentelemetry",
    "trace",
    "visualization",
    "observability",
    "distributed-tracing",
    "waterfall",
    "timeline"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "esbuild": "^0.27.3",
    "typescript": "^5.9.3"
  },
  "files": [
    "dist",
    "src"
  ]
}
